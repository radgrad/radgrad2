{
  "name": "radgrad2",
  "private": true,
  "scripts": {
    "start": "cross-env METEOR_NO_RELEASE_CHECK=1 meteor --settings ../config/settings.development.json --port 3200",
    "start-custom": "cross-env METEOR_NO_RELEASE_CHECK=1 meteor --settings ../custom/settings.development.json --port 3200",
    "start-production": "meteor --production --no-release-check --settings ../config/settings.development.json --port 3200",
    "lint": "eslint --fix --quiet --ext .tsx --ext .ts ./imports",
    "lint-console": "eslint --fix --ext .tsx --ext .ts ./imports",
    "pretest": "npm run lint",
    "test": "cross-env METEOR_NO_RELEASE_CHECK=1 TEST_BROWSER_DRIVER=puppeteer meteor test --once --driver-package meteortesting:mocha --port 3300",
    "test-no-lint": "cross-env METEOR_NO_RELEASE_CHECK=1 TEST_BROWSER_DRIVER=puppeteer meteor test --once --driver-package meteortesting:mocha --port 3300",
    "test-app": "cross-env METEOR_NO_RELEASE_CHECK=1 TEST_BROWSER_DRIVER=puppeteer meteor test --full-app --once --driver-package meteortesting:mocha --port 3300",
    "test-e2e": "testcafe chrome:headless imports/ui/**/*.test-e2e.ts",
    "test-e2e-v": "testcafe chrome imports/ui/**/*.test-e2e.ts",
    "deploy": "DEPLOY_HOSTNAME=galaxy.meteor.com meteor deploy radgrad2.meteorapp.com --settings ../config/settings.production.json --owner radgrad"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.3",
    "acorn": "^7.3.1",
    "bcrypt": "^4.0.1",
    "chart.js": "^2.9.3",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.8.0",
    "core-js": "^3.6.5",
    "cosmiconfig": "^6.0.0",
    "deepmerge": "^4.2.2",
    "graphql": "^15.1.0",
    "highcharts": "^8.1.2",
    "highcharts-react-official": "^3.0.0",
    "history": "^4.10.1",
    "immutable": "^4.0.0-rc.12",
    "meteor-node-stubs": "^1.0.0",
    "moment": "^2.27.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-addons-pure-render-mixin": "^15.6.2",
    "react-beautiful-dnd": "^13.0.0",
    "react-chartjs-2": "^2.9.0",
    "react-content-loader": "^5.1.0",
    "react-datepicker": "^2.16.0",
    "react-dnd": "^10.0.2",
    "react-dnd-html5-backend": "^10.0.2",
    "react-dom": "^16.13.1",
    "react-images-upload": "^1.2.8",
    "react-is": "^16.13.1",
    "react-markdown": "^4.3.1",
    "react-native": "^0.62.2",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scroll-up-button": "^1.6.4",
    "react-slick": "^0.25.2",
    "redux": "^4.0.5",
    "seamless-immutable": "^7.1.4",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2",
    "showdown": "^1.9.1",
    "simpl-schema": "^1.8.0",
    "slick-carousel": "^1.8.1",
    "styled-components": "^5.1.1",
    "sweetalert2": "^9.15.2",
    "sweetalert2-react-content": "^3.0.1",
    "typesafe-actions": "^5.1.0",
    "uniforms": "^2.6.9",
    "uniforms-bridge-simple-schema-2": "^2.6.9",
    "uniforms-semantic": "^2.6.9"
  },
  "devDependencies": {
    "@babel/helper-call-delegate": "^7.10.3",
    "@hapi/joi": "^17.1.1",
    "@react-native-community/cli-debugger-ui": "^4.9.0",
    "@react-native-community/cli-types": "^4.10.1",
    "@types/chai": "^4.2.11",
    "@types/jasmine": "^3.5.11",
    "@types/lodash": "^4.14.157",
    "@types/meteor": "^1.4.47",
    "@types/meteor-roles": "^1.2.32",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.12",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/react-slick": "^0.23.4",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "ansi-escapes": "^4.3.1",
    "art": "^0.10.3",
    "babel-eslint": "^10.1.0",
    "basic-auth": "^2.0.1",
    "chai": "^4.2.0",
    "chromedriver": "^81.0.0",
    "command-exists": "^1.2.9",
    "compression": "^1.7.4",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-meteor": "^7.0.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "faker": "^4.1.0",
    "fast-check": "^1.25.1",
    "fs-extra": "^9.0.1",
    "growly": "^1.3.0",
    "mime-db": "^1.44.0",
    "mime-types": "^2.1.27",
    "mocha": "^7.2.0",
    "morgan": "^1.10.0",
    "node-notifier": "^7.0.1",
    "puppeteer": "^3.3.0",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "testcafe": "^1.8.7",
    "through2": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5",
    "xtend": "^4.0.2"
  },
  "eslintConfig": {
    "settings": {
      "import/extensions": [
        ".js",
        ".jsx",
        ".ts",
        ".tsx"
      ],
      "import/parsers": {
        "@typescript-eslint/parser": [
          ".ts",
          ".tsx"
        ]
      },
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        }
      }
    },
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "meteor",
      "react"
    ],
    "extends": [
      "airbnb",
      "plugin:meteor/recommended",
      "react-app"
    ],
    "rules": {
      "arrow-parens": "off",
      "camelcase": "off",
      "class-methods-use-this": "off",
      "func-names": "off",
      "jsx-a11y/anchor-is-valid": "off",
      "import/extensions": "off",
      "import/no-cycle": "warn",
      "import/no-unresolved": "off",
      "import/no-absolute-path": "off",
      "import/imports-first": "off",
      "import/prefer-default-export": "off",
      "import/no-extraneous-dependencies": "off",
      "indent": "off",
      "linebreak-style": "off",
      "lines-between-class-members": "off",
      "max-len": "off",
      "meteor/eventmap-params": [
        2,
        {
          "eventParamName": "event",
          "templateInstanceParamName": "instance"
        }
      ],
      "meteor/template-names": "off",
      "no-confusing-arrow": [
        "error",
        {
          "allowParens": true
        }
      ],
      "no-console": [
        "warn",
        {
          "allow": [
            "off",
            "error"
          ]
        }
      ],
      "no-plusplus": "off",
      "no-underscore-dangle": "off",
      "no-nested-ternary": "off",
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error",
      "no-useless-constructor": "off",
      "@typescript-eslint/no-useless-constructor": "error",
      "object-curly-newline": "off",
      "object-property-newline": "off",
      "object-shorthand": "off",
      "operator-linebreak": "off",
      "padded-blocks": "off",
      "prefer-arrow-callback": "off",
      "prefer-destructuring": "off",
      "prefer-promise-reject-errors": "off",
      "react/destructuring-assignment": "off",
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            "js",
            ".jsx",
            "ts",
            ".tsx"
          ]
        }
      ],
      "react/jsx-fragments": "off",
      "react/jsx-props-no-spreading": "off",
      "react/prefer-stateless-function": "off",
      "react/sort-comp": "off",
      "react/jsx-one-expression-per-line": "off",
      "react/jsx-closing-tag-location": "off"
    }
  }
}
